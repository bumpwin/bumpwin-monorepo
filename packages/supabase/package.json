{
  "name": "@workspace/supabase",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./src/realtime": "./src/realtime.ts",
    "./src/domain": "./src/domain.ts",
    "./src/error": "./src/error.ts",
    "./src/client": "./src/client.ts"
  },
  "scripts": {
    "clean": "echo 'No build artifacts to clean'",
