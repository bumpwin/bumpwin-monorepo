{
  "name": "monorepo-template",
  "private": true,
  "type": "module",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "pnpm run pm:dev",
    "pm:dev": "pnpm -r --parallel --filter web dev",
    "server:dev": "pnpm -r --filter cmd dev",
    "build": "pnpm run pm:build",
    "pm:build": "pnpm -r --parallel --filter web build",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "format": "biome format . --write",
    "fmt": "pnpm run lint:fix && pnpm run format",
    "typecheck": "bunx tsc -b",
    "pages:build": "pnpm -r --filter web run pages:build",
    "pages:preview": "pnpm -r --filter web run pages:preview",
    "pages:deploy": "pnpm -r --filter web run pages:deploy"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.3",
    "@hono/zod-validator": "^0.5.0",
    "@mysten/sui": "^1.30.1",
    "@questdb/nodejs-client": "^3.0.0",
    "@supabase/supabase-js": "^2.49.8",
    "@workspace/logger": "workspace:*",
    "@workspace/questdb": "workspace:*",
    "@workspace/shadcn": "workspace:*",
    "@workspace/sui": "workspace:*",
    "@workspace/supabase": "workspace:*",
    "bumpwin": "^0.202505.23",
    "hono": "^4.7.10",
    "neverthrow": "^8.2.0",
    "pg": "^8.16.0",
    "ts-pattern": "^5.7.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "wrangler": "^4.16.1",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/next-on-pages": "^1.13.12",
    "@types/bun": "^1.2.14",
    "@types/node": "^22.15.21"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
