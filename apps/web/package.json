{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "config": {
    "NEXT_ON_PAGES_PACKAGE_MANAGER": "pnpm"
  },
  "scripts": {
    "build": "pnpm exec next build",
    "vercel-build": "next build",
    "dev": "next dev",
    "start": "next start",
    "check": "biome check .",
    "format": "biome format . --write",
    "typecheck": "tsc --noEmit",
    "pages:build": "pnpm --filter @workspace/api build && pnpm exec next build && pnpm dlx @cloudflare/next-on-pages@1",
    "pages:preview": "wrangler pages dev .vercel/output/static",
    "pages:deploy": "wrangler pages deploy .vercel/output/static"
  },
  "dependencies": {
    "@mysten/dapp-kit": "^0.16.6",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.80.5",
    "@workspace/api": "workspace:*",
    "@workspace/logger": "workspace:*",
    "@workspace/mockdata": "workspace:*",
    "@workspace/sui": "workspace:*",
    "@workspace/types": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.16.0",
    "gsap": "^3.13.0",
    "lenis": "^1.3.4",
    "lightweight-charts": "^5.0.7",
    "lucide-react": "^0.513.0",
    "mermaid": "^11.6.0",
    "next": "15.3.3",
    "react": "^19.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "reactflow": "^11.11.4",
    "recharts": "^2.15.3",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4.1.8",
    "vercel": "^42.3.0"
  }
}
