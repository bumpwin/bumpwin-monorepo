    "@workspace/mockdata": "workspace:*",
    "@workspace/types": "workspace:*",
    "dotenv": "16.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "20.11.24",
    "bun-types": "1.0.25",
    "typescript": "5.3.3"
  }
